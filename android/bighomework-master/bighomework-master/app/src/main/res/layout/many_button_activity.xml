<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".many_buttonActivity">


    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="446dp"
        android:layout_height="740dp"
        android:src="@drawable/phone119"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.514"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.555" />


    <!--+++++++++++++++++++备份后续用++++++++++++++++++++++++++++-->


    <TextView
        android:id="@+id/usr"
        android:layout_width="276dp"
        android:layout_height="62dp"
        android:layout_marginTop="68dp"
        android:textColor="@color/teal_200"
        android:textSize="23dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.511"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView6"></TextView>

    <TextView
        android:id="@+id/pwd"
        android:layout_width="278dp"
        android:layout_height="63dp"
        android:layout_marginTop="148dp"
        android:textColor="@color/teal_200"

        android:textSize="23dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView6"></TextView>

    <TextView
        android:id="@+id/yzm"
        android:layout_width="277dp"
        android:layout_height="64dp"
        android:layout_marginTop="232dp"
        android:textColor="@color/teal_200"

        android:textSize="23dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.511"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView6"></TextView>


    <Button
        android:id="@+id/button_many_button_activity_browser"
        android:layout_width="110dp"
        android:layout_height="57dp"
        android:layout_marginTop="112dp"
        android:background="#4F06FA"
        android:hint="搜索B站"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.82"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_many_button_activity_open_firstapp" />

    <Button
        android:id="@+id/button_many_button_activity_open_firstapp"
        android:layout_width="120dp"
        android:layout_height="64dp"
        android:layout_marginBottom="32dp"
        android:background="#4F06FA"
        android:hint="打开信息"
        app:layout_constraintBottom_toTopOf="@+id/button_many_button_activity_open_wechat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.814"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button_many_button_activity_open_secondapp"
        android:layout_width="116dp"
        android:layout_height="58dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="360dp"
        android:background="#4F06FA"
        android:hint="打开设置"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.224"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/yanzhengma"
        app:layout_constraintVertical_bias="0.287" />

    <Button
        android:id="@+id/button_many_button_activity_open_wechat"
        android:layout_width="115dp"
        android:layout_height="59dp"
