        android:hint="打开支付宝"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.22"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/yanzhengma"
        app:layout_constraintVertical_bias="0.287" />

    <Button
        android:id="@+id/button_many_button_activity_open_eleme"
        android:layout_width="122dp"
        android:layout_height="58dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="96dp"
        android:background="#4F06FA"
        android:hint="打开饿了么"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.217"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/yanzhengma"
        app:layout_constraintVertical_bias="0.287" />

    <Button
        android:id="@+id/button_many_button_activity_open_meituanwaimai"
        android:layout_width="126dp"
        android:layout_height="51dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="184dp"
        android:background="#4F06FA"
        android:hint="打开美团外卖"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.217"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/yanzhengma"
        app:layout_constraintVertical_bias="0.287" />

    <Button
        android:id="@+id/button_many_button_activity_open_baiduditu"
        android:layout_width="125dp"
        android:layout_height="61dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="360dp"
        android:background="#4F06FA"
        android:hint="打开百度地图"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.808"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/yanzhengma"
        app:layout_constraintVertical_bias="0.287" />


</androidx.constraintlayout.widget.ConstraintLayout>